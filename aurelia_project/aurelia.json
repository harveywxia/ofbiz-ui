{
  "name": "ofbiz-frontend",
  "type": "project:application",
  "paths": {
    "root": "src",
    "resources": "resources",
    "elements": "resources/elements",
    "attributes": "resources/attributes",
    "valueConverters": "resources/value-converters",
    "bindingBehaviors": "resources/binding-behaviors"
  },
  "transpiler": {
    "id": "babel",
    "fileExtension": ".js"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "hmr": false,
    "open": false,
    "port": 6060,
    "host": "localhost",
    "output": "public"
  },
  "name": "aurelia-table",
  "path": "../node_modules/au-table/dist/amd",
  "main": "index",
  "resources": [
    "au-table-pagination.html"
  ],
  "packageManager": "npm"
}

